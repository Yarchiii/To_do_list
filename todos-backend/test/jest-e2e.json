{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "roots": ["<rootDir>/test", "<rootDir>/src"],
  "collectCoverage": true,
  "collectCoverageFrom": [
    "<rootDir>/src/**/*.ts",
    "!<rootDir>/src/main.ts"
  ],
  "coverageDirectory": "<rootDir>/coverage",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {"^.+\\.(t|j)s$": "ts-jest"}
}
